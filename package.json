  "license": "MIT",
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "bugs": {
    "url": "https://github.com/OfficialYoungX/darkmode-hook/issues"
  },
  "homepage": "https://github.com/OfficialYoungX/darkmode-hook#readme",
  "dependencies": {
    "highlight.js": "^9.17.1",
    "parcel-bundler": "^1.12.4"
  },
  "devDependencies": {
    "parcel-plugin-clean-dist": "0.0.6"
  }
}
