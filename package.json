{
  "name": "@yangcongming/darkmode-hook",
  "version": "1.0.1",
  "description": "A js plugin that allows your page to quickly access the darkmode function",
  "main": "index.js",
  "keywords": [
    "darkmode",
    "javascript",
    "simple"
  ],
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build index.html --public-url ./",
    "build:lib": "parcel build ./src/main.js -d lib -o index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OfficialYoungX/darkmode-hook.git"
  },
